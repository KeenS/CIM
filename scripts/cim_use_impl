#!/bin/sh
if [ -L $cim_bin_path/$1 ];then
	LISP_IMPL=`echo $1 | awk -F- '{print \$1}'`
	rm -f $cim_bin_path/$LISP_IMPL
	ln -s $cim_bin_path/$1 $cim_bin_path/$LISP_IMPL
elif cim_which_impl=`which $1`;then
	LISP_IMPL=$cim_which_impl
	unset cim_which_impl
else
	echo "$1 is not installed. Exit."
	return 1
fi
