#!/bin/sh -
# This file is for execute
. "$CIM_HOME/scripts/cim_utils"

while [ $# -gt 0 ];do
    if [ "$1" = "all" ] ;then
	set -- `cim list clean | grep -v '\(Subcommand\|^#\)'`
	continue
    fi
    impl=`__cim_split_impl_version "$target" $1 | tr \  -`
    rm -rf "$CIM_HOME/archives/$impl"
    rm -rf "$CIM_HOME/src/$impl"
    shift
done
